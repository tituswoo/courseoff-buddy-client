{
	"manifest_version": 2,

	"name": "Courseoff Buddy for Georgia Tech",
	"description": "Pull in useful stats like average grades, professor popularity and difficulty, course descriptions, and more for all your Georgia Tech classes on Courseoff.",
	"version": "0.1",

	"page_action": {
		/*"default_icon": {
			"19": "icon.png"
		},*/
		"default_title": "Course Bdudy",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"https://ajax.googleapis.com/",
		"http://*/"
	],
	"content_scripts": [
		{
			"matches": ["https://gatech.courseoff.com/workspace/*"],
			"css": ["main.css"],
			"js": [
				"jquery-1.11.2.min.js",
				"jquery-observe.js",
				"Events.js",
				"utilities.js",
				"main.js"
			]
		}
	],
	"background": {
		"scripts": ["jquery-1.11.2.min.js", "background.js"],
		"persistent": false
	}
}
